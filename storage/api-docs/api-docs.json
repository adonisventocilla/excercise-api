{
    "openapi": "3.0.0",
    "info": {
        "title": "Exercise API",
        "description": "Exercise API for Delfosti",
        "version": "1.0"
    },
    "servers": [{
        "url": "http://exercise-api.test/"
    }],
    "paths": {
        "/api/v1/article?search={search}": {
            "get": {
                "tags": [
                    "Articles"
                ],
                "summary": "Search article by name",
                "description": "Search articles by name and return details with categories.",
                "operationId": "04bf61ded9a1e277bbce4ce85ecf7fbb",
                "parameters": [{
                    "name": "search",
                    "in": "path",
                    "description": "Parameter to search by name",
                    "required": true,
                    "schema": {
                        "type": "string"
                    },
                    "example": "Jaida"
                }],
                "responses": {
                    "200": {
                        "description": "View article's details",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "id": {
                                            "type": "number",
                                            "example": 10
                                        },
                                        "name": {
                                            "type": "string",
                                            "example": "Camioneta Chevrolet"
                                        },
                                        "description": {
                                            "type": "string",
                                            "example": "Camioneta Chevrolet 2010"
                                        },
                                        "status": {
                                            "type": "number",
                                            "example": 1
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "example": "2022-10-10 10:10:10.000000Z"
                                        },
                                        "updated_at": {
                                            "type": "string",
                                            "example": "2022-10-10 10:10:10.000000Z"
                                        },
                                        "categories": {
                                            "type": "array",
                                            "articles": {
                                                "properties": {
                                                    "id": {
                                                        "type": "number",
                                                        "example": 10
                                                    },
                                                    "name": {
                                                        "type": "string",
                                                        "example": "Vehiculo"
                                                    },
                                                    "slug": {
                                                        "type": "string",
                                                        "example": "vehiculo"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error occurred."
                    }
                }
            }
        },
        "/api/v1/articles?qty={qty}": {
            "get": {
                "tags": [
                    "Articles"
                ],
                "summary": "List all articles",
                "description": "List all articles by quantity",
                "operationId": "9f5740316b4416cb27736be7da0c70cd",
                "parameters": [{
                    "name": "qty",
                    "in": "path",
                    "description": "Parameter to set the quantity of articles to return",
                    "required": false,
                    "schema": {
                        "type": "integer"
                    },
                    "example": 10
                }],
                "responses": {
                    "200": {
                        "description": "View article's details",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "id": {
                                            "type": "number",
                                            "example": 10
                                        },
                                        "name": {
                                            "type": "string",
                                            "example": "Camioneta Chevrolet"
                                        },
                                        "description": {
                                            "type": "string",
                                            "example": "Camioneta Chevrolet 2010"
                                        },
                                        "status": {
                                            "type": "number",
                                            "example": 1
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "example": "2022-10-10 10:10:10.000000Z"
                                        },
                                        "updated_at": {
                                            "type": "string",
                                            "example": "2022-10-10 10:10:10.000000Z"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error occurred."
                    }
                }
            }
        },
        "/api/v1/category?search={search}": {
            "get": {
                "tags": [
                    "Category"
                ],
                "summary": "Search category by name",
                "description": "Search categories by name and return details with categories.",
                "operationId": "1cca901a8fb0d96c98c5e7e39279b595",
                "parameters": [{
                    "name": "search",
                    "in": "path",
                    "description": "Parameter to search by name",
                    "required": true,
                    "schema": {
                        "type": "string"
                    },
                    "example": "Petro"
                }],
                "responses": {
                    "200": {
                        "description": "View category's details",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "id": {
                                            "type": "number",
                                            "example": 10
                                        },
                                        "name": {
                                            "type": "string",
                                            "example": "Camioneta Chevrolet"
                                        },
                                        "slug": {
                                            "type": "string",
                                            "example": "camioneta-chevrolet-2010"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error occurred."
                    }
                }
            }
        }
    }
}